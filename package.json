    "css-loader": "1.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-plugin-mocha": "5.0.0",
    "eslint-plugin-react": "7.7.0",
    "expose-loader": "0.7.5",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "mocha": "5.2.0",
    "node-sass": "4.9.3",
    "nodemon": "1.18.4",
    "sass-loader": "7.1.0",
